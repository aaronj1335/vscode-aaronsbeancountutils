{
	"name": "aaronsbeancountutils",
	"displayName": "AaronsBeancountUtils",
	"description": "Utilities for Beancount (i.e. inline calculator)",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/aaronj1335/vscode-aaronsbeancountutils.git"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"main": "./out/extension.js",
	"publisher": "aaronj1335",
	"activationEvents": [
		"onCommand:aaronsbeancountutils.calc"
	],
	"contributes": {
		"commands": [
			{
				"command": "aaronsbeancountutils.calc",
				"title": "Calculate line and copy result"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "aaronsbeancountutils.calc",
					"when": "editorLangId == beancount"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile:calculator": "node_modules/.bin/jison -o src/calculator.js src/calculator.jison",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"jison": "^0.4.18",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vsce": "^1.97.0",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"decimal.js": "^10.3.1"
	}
}